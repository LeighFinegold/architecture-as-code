{
  "name": "docusaurus-docs",
  "templates": [
    {
      "template": "node.mdx.hbs",
      "from": "document.nodes",
      "output": "docs/nodes/{{node.unique-id}}.md",
      "output-type": "repeated",
      "alias": "node",
      "frontmatter": {
        "enabled": true,
        "id": "{{node.unique-id}}",
        "title": "{{node.name}}",
        "architecture": "{{pathToArchitecture}}",
        "node": "nodes['{{node.unique-id}}']"
      }
    }
  ]
}
